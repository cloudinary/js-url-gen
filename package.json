{
  "name": "@cloudinary/base",
  "version": "1.0.0-beta.3",
  "description": "",
  "scripts": {
    "test": "npm run test:types && npm run build && jest --coverage --reporters default &&  npm run test:size",
    "test:size": "ts-node -O '{\"module\":\"CommonJS\"}' __TESTS_BUNDLE_SIZE__/bin/bin.ts",
    "test:comp": "node ./scripts/updateCompliationTests.js && jest __TESTS__/compilation.test.ts  --coverage",
    "test:comp:service": "ts-node ./devTools/sanity/index.ts && jest __TESTS__/compilation.test.ts  --coverage",
    "test:unit": "jest __TESTS__/unit --reporters default",
    "test:unit:watch": "jest __TESTS__/unit --watch --reporters default",
    "test:integration": "jest __TESTS__/integration",
    "test:integration:watch": "jest __TESTS__/integration --watch",
    "test:backcomp": "jest __TESTS__/backwardsComaptibility",
    "test:backcomp:watch": "jest __TESTS__/backwardsComaptibility --watch",
    "build": "bash ./scripts/build.sh",
    "test:types": "tsc --project tsconfig.test.json",
    "build:ESM": "tsc --project tsconfig.json",
    "build:UMD": "rollup -c",
    "build:docs": "node ./scripts/buildDocs.js",
    "build:entryPoints": "ts-node ./scripts/createEntrypoints.ts",
    "build:copyCleanPackageJSON": "ts-node scripts/copyPackageJsonToSrc.ts",
    "lint": "eslint src __TESTS__ --color --ext .ts",
    "start": "rollup -c rollup.dev.config.js -w",
    "bundlewatch": "bundlewatch --config ./bundlewatch.config.js",
    "release-dry-run": "npm run build && cp README.md LICENSE ./dist && cd ./dist && release-it --dry-run",
    "release": "npm run build && cp README.md LICENSE ./dist && cd ./dist && release-it"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@release-it/bumper": "^2.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.2.3",
    "@types/mock-fs": "^4.10.0",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "better-docs": "^2.3.0",
    "bootstrap": "^3.3.7",
    "bundlewatch": "^0.2.6",
    "cheerio": "^0.22.0",
    "clipboard": "^2.0.1",
    "cloudinary-core": "^2.11.4",
    "eslint": "^6.8.0",
    "extend": "^3.0.1",
    "glob": "^7.1.2",
    "grunt": "^1.0.2",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-handlebars": "^1.0.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-jsdoc": "^2.2.0",
    "handlebars": "^4.0.11",
    "handlebars-layouts": "^3.1.4",
    "jest": "^26.0.1",
    "jest-html-reporters": "^1.2.1",
    "jquery": "^3.1.1",
    "jsdoc": "^3.6.4",
    "jsdoc-plugin-typescript": "^2.0.5",
    "lunr": "^1.0.0",
    "mock-fs": "^4.12.0",
    "moment": "^2.22.1",
    "prismjs": "^1.14.0",
    "release-it": "^14.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "sanitize-html": "^1.18.2",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.3",
    "webpack": "^4.44.1",
    "@types/node": "^15.14.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.1"
  },
  "dependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
    "lodash.clonedeep": "^4.5.0"
  }
}
